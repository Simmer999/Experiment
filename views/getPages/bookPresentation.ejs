<section>
    <div class='spacer'>
    </div>  
    <h1 class='purpleBox'>presentationPage</h1> 
    <%- include('../partials/sidenav'); %>
    <div class="purpleBox">
        <ul class="Books">
  
          <H2>All Books</H2>
        <% if (Books.length > 0) { %>
            <% Books.forEach(book => { %>
                <a href='/Books/<%= book._id %>'> 
                   
                <h3 id='title' ><%= book.title %></h3>
                </a>
                <p id='body'><%= book.body %></p>
                
                
                <% }) %>
            <% } else { %>
                <p>There are no books to display.</p>
            <% } %>        

        </ul>
    </div>
    <div class="purpleBox">
        <a href="/dashboard">Home</a>__
        <a href="/users/logout">Logout</a>
    </div> 
</section>